---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Podman Blog</title>
	<link href="http://containers.github.io/podman.io/atom.xml" rel="self"/>
	<link href="http://containers.github.io/podman.io/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://containers.github.io/</id>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://containers.github.io/podman.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://containers.github.io/podman.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
